---
title: Master Page
---

<!DOCTYPE html>
<html class="no-js" lang="en">
	<head>
		{{#block "master-head"}}
			{{>head-meta}}
			{{>head-icons}}
			{{>head-css}}
			{{>head-fonts}}
			{{>head-scripts}}
		{{/block}}
	</head>
	<body>

		{{>header}}

    <div id="page" class="page{{#if pageClass}} {{pageClass}}{{/if}}{{#if visibleGrid}} visible-grid{{/if}}">
			{{#block "master-body"}}
				{{body}}
			{{/block}}
			{{>footer}}
		</div>

    {{#block "master-foot"}}
			{{>foot-scripts}}
		{{/block}}

  </body>
</html>
